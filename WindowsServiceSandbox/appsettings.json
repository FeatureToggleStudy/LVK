{
    "Logging": {
        "Destinations": {
            "Console": {
                "LogLevel": "Information"
            },
            "File": {
                "DirectoryPath": "D:\\Temp",
                "Filename": "ws-{0:yyyy-MM-dd}.log",
                "LogLevel": "Information"
            }
        }
    },
    "WindowsService": {
        "Metadata": {
            "Name": "WindowsServiceSandbox",
